@model IEnumerable<string>

@Html.ActionLink("Inicio","ListaCoches","Coches",null,
new { @class = "btn btn-light" })
@foreach(var link in Model) {
    string controllerName = "";
    string actionName = "";
    if(link == "Vehículos")
    {
        controllerName = "Coches";
        actionName = "ListaCoches";
    }
    else if(link == "Clientes")
    {
        controllerName = "Clientes";
        actionName = "ListaClientes";
    }
    else if(link == "Delegaciones")
    {
        controllerName = "Delegaciones";
        actionName = "ListaDelegaciones";
    }
    else if(link == "Reservas")
    {
        controllerName = "Reservas";
        actionName = "ListaReservas";
    }

    @Html.RouteLink(link,new {
        controller = controllerName,
        action = actionName,
        page = 1
    },new {
        @class = "btn btn-light"
    })
}
